{
    "background": {
       "persistent": false,
       "scripts": [ "js/background.js" ]
    },
   "browser_action": {
      "default_icon": "images/icon-16.png",
      "default_popup": "popup.html",
      "default_title": "Get Informations"
   },
   "options_page": "options.html",
   "content_scripts": [ {
      "js": [ "js/content.js", "js/functions.js", "js/jquery-2.1.1.min.js"],
      "matches": [ "*://*/*", "file://*/*" ]
   } ],
   "description": "Get Informations",
   "icons": {
      "128": "images/icon-128.png",
      "16": "images/icon-16.png"
   },
   "manifest_version": 2,
   "name": "Get Informations",
   "offline_enabled": false,
    "permissions": [
        "tabs",
        "http://*/*",
        "https://*/*"
    ],
   "short_name": "Get Informations",
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.0",
   "web_accessible_resources": [ "images/icon-128.png" ]
}
